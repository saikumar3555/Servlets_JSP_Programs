<web-app>
  <servlet>
     <servlet-name>LoginServlet</servlet-name>
     <servlet-class>com.nit.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>LoginServlet</servlet-name>
      <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
     <servlet-name>RegistrationServlet</servlet-name>
     <servlet-class>com.nit.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>RegistrationServlet</servlet-name>
      <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
     <servlet-name>ViewServlet</servlet-name>
     <servlet-class>com.nit.ViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>ViewServlet</servlet-name>
      <url-pattern>/view</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
     <servlet-name>AddServlet</servlet-name>
     <servlet-class>com.nit.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>AddServlet</servlet-name>
      <url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
     <servlet-name>DeleteServlet</servlet-name>
     <servlet-class>com.nit.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>DeleteServlet</servlet-name>
      <url-pattern>/delete</url-pattern>
  </servlet-mapping>
   




  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
    
  </welcome-file-list>
</web-app>