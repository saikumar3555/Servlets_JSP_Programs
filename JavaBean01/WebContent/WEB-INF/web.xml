<web-app>
     <servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.nit.LoginServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


      <servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>com.nit.RegistrationServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ProductServlet</servlet-name>
		<servlet-class>com.nit.ProductServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/retrive</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DisplayServlet</servlet-name>
		<servlet-class>com.nit.DisplayServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayServlet</servlet-name>
		<url-pattern>/dis</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Login.html</welcome-file>
	</welcome-file-list>

</web-app>